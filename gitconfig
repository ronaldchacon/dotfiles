[user]
  # name = name
  # email = me@example.com
[color]
  ui = true
[push]
  default = simple
[alias]
  ec = config --global -e
  la = "!git config -l | ag alias | cut -c 7-"
  up = !git pull --rebase --prune
  co = checkout
  cob = checkout -b
  cm = !git add -A && git commit -m
  save = !git add -A && git commit -m 'SAVEPOINT'
  undo = reset HEAD~1 --mixed
  amend = commit -a --amend
  bm = branch --merged
  bd = branch -d
  cl = clone
  cp = cherry-pick
  st = status
  diff = diff --word-diff
  ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
  ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --shortstat
[core]
  excludesfile = ~/.gitignore
  editor = vim
